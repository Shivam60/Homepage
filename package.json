{
  "name": "react-ts-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:dev": "./node_modules/.bin/webpack --config webpack.config.js --mode development",
    "start:dev": "npm run build:dev && node server.js",
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "unit-tests:watch": "jest --watch",
    "prettier:check": "prettier --check \"./**/*.{ts,tsx}\" && npm run tslint",
    "prettier:fix": "prettier --write \"./**/*.{ts,tsx}\"",
    "pFix": "npm run prettier:fix && npm run tFix",
    "tFix": "tslint -p tsconfig.json -c tslint.json --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "husky": "^4.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run prettier:check"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-links": "^5.3.9",
    "@storybook/addons": "^5.3.9",
    "@storybook/react": "^5.3.9",
    "@types/react": "*",
    "@types/react-dom": "*",
    "babel-loader": "^8.0.6",
    "html-webpack-plugin": "*",
    "source-map-loader": "*",
    "ts-loader": "*",
    "typescript": "*",
    "webpack": "*",
    "lint-staged": "^9.5.0",
    "webpack-cli": "*",
    "webpack-dev-middleware": "*",
    "webpack-hot-middleware": "*"
  }
}
